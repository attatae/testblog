<h1>All Startups</h1>
<hr>

<table class="table">
  <thead>
    <tr>
      <th>About</th>
      <th>Location</th>
      <th>Keywords</th>
    </tr>
  </thead>

  <tbody>
    <% @startups.each do |startup| %>
      <tr>
        <td><%= startup.headline %></td>
        <td><%= startup.city %></td>
        <td><%= startup.skills %></td>
      </tr>
    <% end %>
  </tbody>
</table>

